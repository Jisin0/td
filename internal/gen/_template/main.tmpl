{{- /*gotype: github.com/gotd/td/internal/gen.config*/ -}}

{{ define "main" }}
{{ $pkg := $.Package }}
{{ template "header" $ }}

{{ range $s := $.Structs }}
{{ template "struct" $s }}
{{ template "zero_derive" $s }}
{{ template "string_derive" $s }}
{{ template "fill_from" $s }}
{{ template "type_info" $s }}
{{ template "encode" $s }}
{{ template "decode" $s }}
{{ template "getset" $s }}
{{ template "field_mapping" newStructConfig ($s) ($) }}
{{ template "method" newStructConfig ($s) ($) }}
{{ end }}

{{ range $f := $.Interfaces }}
{{ template "interface" newInterfaceConfig ($f) ($) }}
{{ template "interface_mapping" $f }}
{{ template "box" $f }}
{{ end }}

{{ template "slices" $ }}
{{ end }}
